<!-- Create.cshtml para registar um veículo -->
@model Garage88.Data.Entities.Vehicle

<form asp-action="Create" method="post">
    <div>
        <label>Modelo</label>
        <input asp-for="Model" />
    </div>
    <div>
        <label>Cliente</label>
        <select asp-for="ClientId" asp-items="ViewBag.Clients"></select>
    </div>
    <button type="submit">Guardar</button>
</form>
